//Objeto Endereço

//Criar um objeto endereço que contem: Rua, Cidade, CEP e exibir


let endereco = {
    rua: 'Av Paulista',
    cidade: 'São Paulo',
    cep: '05836-120'
};

function exibirEndereco(endereco){
    for (let chave in endereco)
    console.log(chave, endereco[chave]);
}

exibirEndereco(endereco);